components:
  schemas:
    AIAgent:
      type: object
      properties:
        id:
          type: integer
          readOnly: true
          description: |-
            Agent ID.
            Example: `2142`
        project:
          type: integer
          description: The project this agent belongs to
        organization:
          type: integer
          writeOnly: true
          description: The organization this agent belongs to
        agent_name:
          type: string
          description: "\nA descriptive name for your agent\nExample: \n- `\"Customer\
            \ Service Assistant\"`\n- `\"Sales Bot\"`\n"
          maxLength: 255
        contact_number:
          type: string
          description: |2

            Phone number assigned to this agent for calls (must start with + followed by digits, min 8 chars)
            Example: `"+1234567890"`
          maxLength: 30
        inbound:
          type: boolean
          description: |
            Whether this agent handles inbound calls.
            - If true, our agent will call you on above number.
            - If false, your agent will have to call our agent.

            Example: `true` or `false`
        description:
          type: string
          description: "\nA detailed description of what this agent does and how it\
            \ should interact\nExample: \n```text\nAI agent for handling customer\
            \ support inquiries and resolving technical issues\n```\n"
        language:
          enum:
          - ar
          - bg
          - zh
          - cs
          - da
          - nl
          - en
          - fi
          - fr
          - de
          - el
          - hi
          - he
          - hu
          - id
          - it
          - ja
          - ko
          - ms
          - multi
          - 'no'
          - pl
          - pt
          - ro
          - ru
          - sk
          - es
          - sv
          - tr
          - ta
          - uk
          - vi
          type: string
          x-spec-enum-id: 47057a83d036d342
          default: en
          description: |2-

            The primary language this agent uses for communication (e.g. 'en' for English)


            * `ar` - Arabic
            * `bg` - Bulgarian
            * `zh` - Chinese Simplified
            * `cs` - Czech
            * `da` - Danish
            * `nl` - Dutch
            * `en` - English
            * `fi` - Finnish
            * `fr` - French
            * `de` - German
            * `el` - Greek
            * `hi` - Hindi
            * `he` - Hebrew
            * `hu` - Hungarian
            * `id` - Indonesian
            * `it` - Italian
            * `ja` - Japanese
            * `ko` - Korean
            * `ms` - Malay
            * `multi` - Multilingual
            * `no` - Norwegian
            * `pl` - Polish
            * `pt` - Portuguese
            * `ro` - Romanian
            * `ru` - Russian
            * `sk` - Slovak
            * `es` - Spanish
            * `sv` - Swedish
            * `tr` - Turkish
            * `ta` - Tamil
            * `uk` - Ukrainian
            * `vi` - Vietnamese
        assistant_id:
          type:
          - string
          - 'null'
          description: |2

            External assistant service identifier (minimum 10 characters)
            Example: `"asst_abc123xyz"`
          maxLength: 255
        chat_assistant_id:
          type: string
          description: |2

            Chat-specific assistant service identifier (minimum 10 characters)
            Example: `"chat_asst_abc123xyz"`
          maxLength: 255
        websocket_url:
          type: string
          description: |2

            WebSocket endpoint URL for real-time communication
            Example: `"wss://api.example.com/ws"`
        websocket_headers:
          type: object
          additionalProperties: {}
          description: |2

            Headers to be sent to the websocket server
        llm_system_prompt:
          type: string
          description: "\nSystem prompt that defines the agent's behavior and personality\n\
            Example: \n```text\nYou are a helpful customer service agent. Always be\
            \ polite and professional.\n```\n"
        llm_model:
          enum:
          - gpt-4o
          - gpt-4o-mini
          - gpt-4.1
          - gpt-4.1-mini
          - claude-sonnet-4-5-20250929
          type: string
          x-spec-enum-id: 50e3b52be1ccac3f
          default: gpt-4o
          description: |2-

            The LLM model to use for simulating user responses


            * `gpt-4o` - gpt-4o
            * `gpt-4o-mini` - gpt-4o-mini
            * `gpt-4.1` - gpt-4.1
            * `gpt-4.1-mini` - gpt-4.1-mini
            * `claude-sonnet-4-5-20250929` - claude-sonnet-4-5
        llm_temperature:
          type: number
          format: double
          default: 0.0
          description: |2

            Controls randomness in responses (0.0-2.0, lower = more focused)
            Example: `0.5`
        llm_max_tokens:
          type: integer
          default: 4096
          description: |2

            Maximum length of simulated responses in tokens
            Example: `4096`
        outbound_auto_call:
          type: boolean
          default: false
          description: |2

            Whether to automatically call the outbound number
            Example: `False` or `True`
        predefined_metrics:
          writeOnly: true
          description: predefined metrics to use for the agent
        knowledge_base_files:
          type: string
          readOnly: true
          description: |2

            List of knowledge base files attached to the agent
            Example: `[1, 2, 3]`
        transcript_provider:
          enum:
          - vapi
          - retell
          - vocera
          - synthflow
          - elevenlabs
          - bland
          - livekit
          - pipecat
          - koreai
          - custom
          type: string
          x-spec-enum-id: aa22baddf041a6c8
          default: vocera
          description: |2-

            Service provider for speech-to-text transcription


            * `vapi` - Vapi
            * `retell` - Retell
            * `vocera` - Vocera
            * `synthflow` - Synthflow
            * `elevenlabs` - Elevenlabs
            * `bland` - Bland
            * `livekit` - Livekit
            * `pipecat` - Pipecat
            * `koreai` - Kore
            * `custom` - Custom
        assistant_provider:
          enum:
          - vapi
          - retell
          - elevenlabs
          - vocera
          - sms
          - self_hosted
          - agentforce
          type: string
          x-spec-enum-id: b0b9a6026e751285
          default: self_hosted
          description: |2-

            Service provider for the AI assistant functionality


            * `vapi` - Vapi
            * `retell` - Retell
            * `elevenlabs` - Elevenlabs
            * `vocera` - Vocera
            * `sms` - Sms
            * `self_hosted` - Self Hosted
            * `agentforce` - Agentforce
        vapi_api_key:
          type: string
          description: |2

            API key for VAPI service provider
            Example: `"vapi_api_key_123"`
        retell_api_key:
          type: string
          description: |2

            API key for Retell service provider
            Example: `"retell_api_key_123"`
        elevenlabs_api_key:
          type: string
          description: |2

            API key for ElevenLabs voice synthesis service
            Example: `"elevenlabs_api_key_123"`
        bland_api_key:
          type: string
          description: |2

            API key for Bland service provider
            Example: `"bland_api_key_123"`
        livekit_api_key:
          type: string
          description: |2

            API key for LiveKit service provider
            Example: `"livekit_api_key_123"`
        livekit_data:
          type: string
          description: |2

            LiveKit additional data (api_secret, url, and config)
        pipecat_api_key:
          type: string
          description: |2

            API key for Pipecat/Daily.co service provider
            Example: `"pipecat_api_key_123"`
        pipecat_data:
          type: string
          description: |2

            Pipecat/Daily.co additional data (webhook_url and config)
        koreai_client_secret:
          type: string
          description: |2

            Client secret key for Kore.ai service provider
            Example: `"client_secret_key_123"`
        koreai_data:
          type: string
          description: |2

            Kore.ai additional data - client_id, bot_id, and host (optional)
        agentforce_client_secret:
          type: string
          description: |2

            Client secret for Salesforce Agentforce OAuth2 authentication
            Example: `"3EG7HHH..."`
        agentforce_data:
          type: string
          description: |2

            Agentforce additional data - client_id, domain, and agent_id
            Example: `{"client_id": "sd7a8...", "domain": "https://example.sandbox.my.salesforce.com", "agent_id": "0Xx..."}`
        custom_provider_api_key:
          type: string
          description: |2

            API key for custom service provider
            Example: `"custom_api_key_123"`
        custom_provider_data:
          type: string
          description: |2

            Additional configuration data for custom service provider
        synthflow_api_key:
          type: string
          description: |2

            API key for Synthflow service provider
