    get:
      operationId: observability_v1_call_logs_list
      parameters:
      - name: page
        required: false
        in: query
        description: A page number within the paginated result set.
        schema:
          type: integer
      - name: page_size
        required: false
        in: query
        description: Number of results to return per page.
        schema:
          type: integer
      - in: query
        name: timestamp_from
        schema:
          type: string
          format: date-time
        description: |2

          Filter results from this timestamp (e.g. 2025-01-01T00:00:00Z)
          Example: `2025-01-01T00:00:00Z`
      - in: query
        name: timestamp_to
        schema:
          type: string
          format: date-time
        description: |2

          Filter results to this timestamp (e.g. 2025-01-31T23:59:59Z)
          Example: `2025-01-31T23:59:59Z`
      - in: query
        name: call_ids
        schema:
          type: string
          minLength: 1
        description: |2

          Comma-separated list of call log IDs
          Example: `1,2,3`
      - in: query
        name: agent_id
        schema:
          type: integer
        description: |2

          Filter by agent ID
          Example: `2142`
        required: true
      - in: query
        name: assistant_id
        schema:
          type: string
          minLength: 1
        description: |2

          Filter by assistant ID
          Example:
          - `"asst_1234567890"`
          - `"agent_xyz789"`
      tags:
      - observability
      security:
      - api_key: []
      responses:
